<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="ingest">
    <target name="ingest">
        <!-- Create an ingest object if any-->
        <echo>Configuring ingest object with name '${holding}-ingest'</echo>
        <ia-task self="ingestSelf">
            <object typeAlias="ingest" checkExistSpEL="?[name=='${holding}-ingest']">
                <create>
                    <name>${holding}-ingest</name>
                </create>
                <content format="xml" file="${basedir}/resources/content/ingest.xml"/>
            </object>
        </ia-task>
    </target>
</project>
