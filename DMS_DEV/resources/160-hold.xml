<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="hold">
    <target name="hold">
        <!-- Create a Hold object if any-->
        <echo>Configuring hold object with name '${holding}-hold'</echo>
        <ia-task self="holdSelf">
            <object typeAlias="hold" checkExistSpEL="?[name=='${holding}-hold']">
                <create>
                    <name>${holding}-hold</name>
                    <holdType>
                        <type>LEGAL</type>
                    </holdType>
                </create>
            </object>
        </ia-task>
    </target>
</project>
