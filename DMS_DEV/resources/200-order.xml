<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="order">
    <target name="order">
        <!-- Create Order object if any-->
        <echo>Configuring an order object with name '${holding}-order'</echo>
        <ia-task self="orderSelf">
            <object typeAlias="order">
                <create checkExistSpEL="?[name=='${holding}-order']">
                    <name>${holding}-order</name>
                </create>
                <update>
                    <priority>1</priority>
                    <retentionPolicy/>
                    <duration>86400</duration>
                </update>
            </object>
        </ia-task>
    </target>
</project>
