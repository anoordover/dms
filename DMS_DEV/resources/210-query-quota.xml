<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="query-quota">
    <target name="query-quota">
        <!-- Create a Query Quota object if any-->
        <echo>Configuring query-quota object with name '${holding}-quota'</echo>
        <ia-task self="qqSelf">
            <object typeAlias="query-quota" checkExistSpEL="?[name=='${holding}-quota']">
                <create>
                    <name>${holding}-quota</name>
                </create>
                <update>
                    <aipQuota>0</aipQuota>
                    <aiuQuota>0</aiuQuota>
                    <dipQuota>0</dipQuota>
                </update>
            </object>
        </ia-task>
    </target>
</project>
